{
  "name": "foo",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: express-to-aws-lambda",
  "customName": false,
  "customRole": false,
  "handler": "foo/handler.crudFoo",
  "timeout": 3,
  "memorySize": 256,
  "authorizer": {},
  "custom": {
    "optimize": {
      "includePaths": [
        "foo/lib"
      ]
    },
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "foo/{id}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "authorization",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${successResponse}"
    },{
      "path": "foo",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "authorization",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${createdResponse}"
    },{
      "path": "foo/{id}",
      "method": "PUT",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "authorization",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${successResponse}"
    }
  ],
  "events": [],
  "environment": "$${functionEnvironment}",
  "vpc": "$${functionVpc}"
}
