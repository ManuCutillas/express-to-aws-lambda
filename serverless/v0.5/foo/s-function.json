{
  "name": "foo",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: express-to-aws-lambda",
  "customName": false,
  "customRole": false,
  "handler": "foo/handler.crudFoo",
  "timeout": 6,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "optimize": {
      "includePaths": [
        "foo/lib"
      ]
    },
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "foo/{id}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponseTemplate}"
    },{
      "path": "foo",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponseTemplate}"
    },{
      "path": "foo/{id}",
      "method": "PUT",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": "$${apiGatewayResponseTemplate}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
