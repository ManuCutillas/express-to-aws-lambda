{
  "name": "authorization",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: express-to-aws-lambda",
  "customName": false,
  "customRole": false,
  "handler": "auth/authorization/handler.handler",
  "timeout": 3,
  "memorySize": 256,
  "authorizer": {
    "type": "TOKEN",
    "identitySource": "method.request.header.Authorization",
    "authorizerResultTtlInSeconds": "0"
  },
  "custom": {
    "optimize": {
      "includePaths": [
        "auth/lib"
      ]
    },
    "excludePatterns": []
  },
  "endpoints": [],
  "events": [],
  "environment": "$${functionEnvironment}",
  "vpc": "$${functionVpc}"
}
